group("boot") {
  if (target_cpu == "x86") {
    deps = [ "x86:bootx86" ]
  } else if (target_cpu == "arm32") {
    deps = [ "arm32:bootarm" ]
  } else if (target_cpu == "arm64") {
    deps = [ "arm64:bootarm64" ]
  }
}
